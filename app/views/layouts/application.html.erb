<script type="text/javascript">
	function calculos(){
		var tipo = parseFloat(document.getElementById("tipo").value);
		console.log("valor tipo "+tipo);
		var tipo2 = tipo/100;
		var valor_crediticio = document.getElementById("valor_credito").value;
		console.log("valor credito "+valor_crediticio);
		var plazo_credito = document.getElementById("plazo_credito").value;
		console.log("plazo_credito "+plazo_credito);
		var resultado = 1+tipo2*plazo_credito*valor_crediticio;
		console.log("resultado "+resultado);
		document.getElementById("resultado").value = resultado;
	}
	function calculos2(){
		var tipo = parseFloat(document.getElementById("tipo2").value);
		console.log("valor tipo "+tipo);
		var tipo2 = tipo/100;
		var valor_crediticio = document.getElementById("valor_credito2").value;
		console.log("valor credito "+valor_crediticio);
		var plazo_credito = document.getElementById("plazo_credito2").value;
		console.log("plazo_credito "+plazo_credito);
		var resultado = 1+tipo2*plazo_credito*valor_crediticio;
		console.log("resultado "+resultado);
